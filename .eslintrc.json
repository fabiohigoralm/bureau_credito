{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules": {
    "camelcase":"off",
    "no-underscore-dangle":"off",
    "no-undef":"off",
    "no-console":"off",
    "no-unused-vars":"off"
  }
}
